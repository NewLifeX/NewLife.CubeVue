{
  "name": "@newlifex/cube-ui",
  "version": "0.1.0",
  "description": "NewLife.Cube New UI",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --no-clean --target lib --dest ../lib --name CubeUI ./src/index.js",
    "publish": "yarn lib && npm publish --access public --registry=https://registry.npmjs.com/"
  },
  "files": [
    "lib/*.js",
    "lib/*.css"
  ],
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^1.1.0-beta.24",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.2.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "authors": [
    "笑笑 <xxred0324@qq.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/NewLifeX/newlife-cube-vueui.git"
  }
}