{
  "name": "@newlifex/cube-ui",
  "version": "0.2.0-beta.0",
  "description": "NewLife.Cube New UI",
  "scripts": {
    "start": "vue-tsc --help",
    "serve": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly --outDir ./dist/types  && vite build --outDir ./dist --emptyOutDir=false",
    "lib": "vue-cli-service build --no-clean --target lib --dest ./demo/lib --name CubeUI ./src/index.ts",
    "publish": "npm publish --access public --registry=https://registry.npmjs.com/"
  },
  "files": [
    "dist"
  ],
  "main": "dist/CubeUI.js",
  "style": "dist/style.css",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.3",
    "@ant-design/icons-vue": "^6.1.0",
    "@element-plus/icons-vue": "^2.1.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "element-plus": "^2.2.32",
    "lodash": "^4.17.21",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.7",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "typescript": "^4.5.2",
    "vite": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "authors": [
    "笑笑 <xxred0324@qq.com>"
